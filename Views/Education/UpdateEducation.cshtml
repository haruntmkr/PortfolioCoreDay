
@model Education
@{
	ViewData["Title"] = "UpdateExperience";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h4 class="page-title">Eğitim Güncelleme</h4>

<div class="content">
	<div class="container-fluid">

		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Eğitim Güncelleme Sayfası</div>
					</div>
					<div class="card-body">

						<form method="post">
							<input type="hidden" asp-for="EducationId" />
							<br />
							<label>Bölüm Adı:</label>
							<input type="text" asp-for="Title" class="form-control" style="border:1px solid black" />
							<br />
							<label>Üniversite </label>
							<input type="text" asp-for="University" class="form-control" style="border:1px solid black" />
							<br />
							<label>Departman</label>
							<input type="text" asp-for="Departmant" class="form-control" style="border:1px solid black" />
							<br />
							<label>Tarih Aralığı</label>
							<textarea rows="4" class="form-control" asp-for="EducationDate" class="form-control" style="border:1px solid black"></textarea>
							<br />
							<button class="btn btn-primary">Eğitimi Güncelle</button>
						</form>

					</div>
				</div>


			</div>
		</div>
	</div>
</div>

